tainer\">\n        {\n            alert == true\n            ? <div className={\"alert alert-warning end\" + fade}>\n                2초이내 구매시 할인\n            </div>\n            : null\n        }\n        {count}\n        <button onClick={()=>{setCount(count + 1)}}>button</button>\n        {/* <yellowBtn bg=\"blue\">btn</yellowBtn> */}\n        <input type=\"text\" onChange={(e)=>{\n            if(isNaN(e.target.value) == true){\n                alert('그러지마세요')\n            }\n            // setNum(e.target.value)\n        }}/>\n        <div className=\"row\">\n            <div className=\"col-md-6\">\n            <img src=\"https://codingapple1.github.io/shop/shoes1.jpg\" width=\"100%\" />\n            </div>\n            <div className=\"col-md-6\">\n            <h4 className=\"pt-5\">{shoes.title}</h4>\n            <p>{shoes.content}</p>\n            <p>{shoes.price}</p>\n            <button className=\"btn btn-danger\" onClick={()=>{\n                dispatch(addItem(shoes))\n            }}>주문하기</button> \n            </div>\n        </div>\n        </div> \n    )\n}\n\nexport default Detail","/Users/kimhaneui/Documents/리액트 프로젝트/shop/src/routes/Cart.js",["111"],[],"import { Table } from 'react-bootstrap'\nimport { useDispatch, useSelector } from 'react-redux'\nimport {countIncrease, increase} from './../store'\nimport {useMemo, memo } from 'react'\nlet Child = memo(function(){\n  return <div>child</div>\n})\nfunction 함수(){\n  return <div>반복문10억번돌린결과</div>\n}\n\nfunction Cart(){\n    let result = useMemo(()=>{ return 함수() }, [])\n    let a = useSelector((state)=>{return state})\n    let dispatch = useDispatch()\n    return (\n        <div>\n          <Child/>\n            <h6>{a.user.name} {a.user.age}의 장바구니</h6>\n            <button onClick={()=>{dispatch(increase())}}>button</button>\n            <Table>\n            <thead>\n                <tr>\n                <th>#</th>\n                <th>상품명</th>\n                <th>수량</th>\n                <th>변경하기</th>     \n                </tr>\n            </thead>\n            {\n            a.cart.map(function(item, i){\n              return(\n                <tbody>\n                <tr>\n                <td>{i}</td>\n                <td>{item.name}</td>\n                <td>{item.count}</td>\n                <td>\n                    <button onClick={()=>{\n                        dispatch(countIncrease(i))\n                    }}> + </button>\n                </td>\n                </tr>\n                </tbody>\n              )\n            })\n          }\n            </Table> \n        </div>\n    )\n}\nexport default Cart","/Users/kimhaneui/Documents/리액트 프로젝트/shop/src/store.js",[],[],"/Users/kimhaneui/Documents/리액트 프로젝트/shop/src/store/userSlice.js",["112"],[],"import { configureStore, createSlice } from '@reduxjs/toolkit'\n\nlet user = createSlice({\n  name : 'user',\n  initialState : {name: 'kim', age: 20} ,\n  reducers: {\n      changeName(state){\n        state.name = 'park' //store의 state 변경하는 법\n      },\n      increase(state, action){\n        state.age += action.payload\n      }\n  }\n})\n\nexport default user","/Users/kimhaneui/Documents/GitHub/react-shop/src/index.js",["113"],[],"/Users/kimhaneui/Documents/GitHub/react-shop/src/reportWebVitals.js",[],[],"/Users/kimhaneui/Documents/GitHub/react-shop/src/App.js",["114","115","116","117","118","119","120","121","122","123","124","125"],[],"/Users/kimhaneui/Documents/GitHub/react-shop/src/store.js",[],[],"/Users/kimhaneui/Documents/GitHub/react-shop/src/data.js",[],[],"/Users/kimhaneui/Documents/GitHub/react-shop/src/routes/Cart.js",["126","127","128"],[],"/Users/kimhaneui/Documents/GitHub/react-shop/src/routes/Detail.js",["129","130","131","132","133","134","135","136","137","138","139","140","141"],[],"/Users/kimhaneui/Documents/GitHub/react-shop/src/store/userSlice.js",["142"],[],"/Users/kimhaneui/Documents/GitHub/react-shop/src/exercise.js",[],[],{"ruleId":"143","severity":1,"mess